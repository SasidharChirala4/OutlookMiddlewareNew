{
  "StoragePath": "#{appsettings.outlookmiddleware.storagepath}#",
  "EdreamsExtensibilityUrl": "#{appsettings.outlookmiddleware.edreamsextensibilityurl}#",
  "EdreamsTokenKey": "#{appsettings.outlookmiddleware.edreamstokenkey}#",
  "EdreamsTokenValue": "#{appsettings.outlookmiddleware.edreamstokenvalue}#",
  "MaxNumberPendingCategories": "#{appsettings.outlookmiddleware.maxnumberpendingcategories}#",
  "ServiceBusConnectionString": "Endpoint=#{servicebus.endpoint}#;SharedAccessKeyName=#{servicebus.sharedaccess.keyname}#;SharedAccessKey=#{servicebus.sharedaccess.key}#",
  "ServiceBusQueueName": "#{appsettings.outlookmiddleware.serviceBusQueueName}#",
  "TransactionSchedulingIntervalInSeconds": "#{appsettings.outlookmiddleware.appSettingsInterval}#",
  "ExchangeAuthority": "#{appsettings.exchange.authority}#",
  "ExchangeResourceId": "#{appsettings.exchange.resourceid}#",
  "ExchangeClientId": "#{appsettings.exchange.clientid}#",
  "ExchangeOnlineServer": "#{appsettings.exchange.onlineserver}#",
  "SharedMailBoxUserNameSecret": "#{appsettings.outlookmiddleware.sharedmailboxusername}#",
  "SharedMailBoxPasswordSecret": "#{appsettings.outlookmiddleware.sharedmailboxpassword}#",
  "KeyVaultUri": "#{appsettings.keyvault.url}#",
  "KeyVaultTenantId": "#{appsettings.keyvault.tenantId}#",
  "KeyVaultClientId": "#{appsettings.keyvault.clientId}#",
  "KeyVaultClientSecret": "#{appsettings.keyvault.secret}#",
  "PreloadedFilesExpiryInMinutes": "#{appsettings.outlookmiddleware.preloadedfilesexpiryinminutes}#",
  "TransactionHistoryExpiryInMinutes": "#{appsettings.outlookmiddleware.transactionhistoryexpiryinminutes}#",
  "ExpirationWorkerIntervalInSeconds": "#{appsettings.outlookmiddleware.expirationworkerintervalinseconds}#",
  "CleanupWorkerIntervalInSeconds": "#{appsettings.outlookmiddleware.cleanupworkerintervalinseconds}#",
  "PreloadedFilesWorkerScheduleStartTime": "#{appsettings.outlookmiddleware.preloadedfilesworkerschedulestarttime}#",
  "PreloadedFilesWorkerScheduleStopTime": "#{appsettings.outlookmiddleware.preloadedfilesworkerschedulestoptime}#",
  "TransactionsWorkerScheduleStartTime": "#{appsettings.outlookmiddleware.transactionsworkerschedulestarttime}#",
  "TransactionsWorkerScheduleStopTime": "#{appsettings.outlookmiddleware.transactionsworkerschedulestoptime}#",
  "CategorizationWorkerScheduleStartTime": "#{appsettings.outlookmiddleware.categorizationworkerschedulestarttime}#",
  "CategorizationWorkerScheduleStopTime": "#{appsettings.outlookmiddleware.categorizationworkerschedulestoptime}#",
  "CategorizationExpiryInMinutes": "#{appsettings.outlookmiddleware.categorizationexpiryinminutes}#",
  "ConnectionStrings": {
    "OutlookMiddlewareDbConnectionString": "Data Source=#{connectionstring.outlookmiddleware.server}#;Initial Catalog=#{connectionstring.outlookmiddleware.database}#;Integrated Security=True;MultipleActiveResultSets=true",
    "OutlookMiddlewarePreloadDbConnectionString": "Data Source=#{connectionstring.outlookmiddleware.preloadserver}#;Initial Catalog=#{connectionstring.outlookmiddleware.preloaddatabase}#;Integrated Security=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": "Debug"
  }
}