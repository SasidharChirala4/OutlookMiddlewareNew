{
  "ServiceName": "#{appsettings.outlookmiddleware.servicename}#",
  "StoragePath": "#{appsettings.outlookmiddleware.storagepath}#",
  "EdreamsExtensibilityUrl": "#{appsettings.outlookmiddleware.edreamsextensibilityurl}#",
  "EdreamsTokenKey": "#{appsettings.outlookmiddleware.edreamstokenkey}#",
  "EdreamsTokenValue": "#{appsettings.outlookmiddleware.edreamstokenvalue}#",
  "MaxNumberPendingCategories": "#{appsettings.outlookmiddleware.maxnumberpendingcategories}#",
  "ServiceBusConnectionString": "Endpoint=#{servicebus.endpoint}#;SharedAccessKeyName=#{servicebus.sharedaccess.keyname}#;SharedAccessKey=#{servicebus.sharedaccess.key}#",
  "ServiceBusQueueName": "#{servicebus.queuename}#",
  "TransactionSchedulingIntervalInSeconds": "#{appsettings.outlookmiddleware.appSettingsInterval}#",
  "ConnectionStrings": {
    "OutlookMiddlewareDbConnectionString": "Data Source=#{connectionstring.outlookmiddleware.server}#;Initial Catalog=#{connectionstring.outlookmiddleware.database}#;Integrated Security=True;MultipleActiveResultSets=true",
    "OutlookMiddlewarePreloadDbConnectionString": "Data Source=#{connectionstring.outlookmiddleware.preloadserver}#;Initial Catalog=#{connectionstring.outlookmiddleware.preloaddatabase}#;Integrated Security=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": "Debug"
  }
}
